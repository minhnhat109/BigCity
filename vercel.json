{
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static-build",
        "config": { "zeroConfig": true }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "/public/index.php"
      }
    ]
  }
  